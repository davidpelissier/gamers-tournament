<p id="notice"><%= notice %></p>

<h1>Listing Bos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phase</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bos.each do |bo| %>
      <tr>
        <td><%= bo.name %></td>
        <td><%= bo.phase %></td>
        <td><%= link_to 'Show', bo %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>